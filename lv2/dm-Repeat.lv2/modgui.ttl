@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/davemollen/dm-Repeat>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-dm-repeat.html> ;
        modgui:stylesheet <modgui/stylesheet-dm-repeat.css> ;
        modgui:screenshot <modgui/screenshot-dm-repeat.png> ;
        modgui:thumbnail <modgui/thumbnail-dm-repeat.png> ;
        modgui:brand "DM" ;
        modgui:label "Repeat" ;
        modgui:model "boxy" ;
        modgui:panel "4-knobs" ;
        modgui:color "cyan" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "freq" ;
            lv2:name "Freq" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "repeats" ;
            lv2:name "Repeats" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "feedback" ;
            lv2:name "Feedback" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "skew" ;
            lv2:name "Skew" ;
        ] ;
    ] .